class LoginWindow(QWidget):
    def __init__(self):
        super().__init__()
        self.initUI()
        # è®¾ç½®å…è®¸ä½¿ç”¨çš„æˆªæ­¢æ—¶é—´ (YYYY-MM-DD HH:MM:SS)
        self.expiration_date_str = "2026-06-21 23:59:59"  # è½¯ä»¶è¿‡æœŸæ—¶é—´
        self.expiration_date = datetime.strptime(self.expiration_date_str, "%Y-%m-%d %H:%M:%S")

        # æ£€æŸ¥æ˜¯å¦éœ€è¦æé†’
        remaining_days = self.get_remaining_days()
        if remaining_days > 0 and remaining_days <= 7:
            QMessageBox.warning(self, "è½¯ä»¶å³å°†æ›´æ–°",
                                f"æœ¬è½¯ä»¶å°†åœ¨{remaining_days}å¤©åŽè‡ªåŠ¨æ›´æ–°ï¼š\nè¯·è”ç³»ã€Šå¯Œè´µã€‹èŽ·å–æœ€æ–°ç‰ˆæœ¬ã€‚")

        # æ£€æŸ¥æ˜¯å¦è¿‡æœŸ
        if self.is_expired():
            QMessageBox.critical(self, "è½¯ä»¶å·²æ›´æ–°", f"ä½ çš„åº”ç”¨ç‰ˆæœ¬è¿‡ä½Žï¼š\nè¯·è”ç³»ã€Šå¯Œè´µã€‹èŽ·å–æœ€æ–°ç‰ˆæœ¬ã€‚")
            sys.exit(0)  # é€€å‡ºç¨‹åº

    def is_expired(self):
        """æ£€æŸ¥å½“å‰æ—¶é—´æ˜¯å¦è¶…è¿‡æŒ‡å®šçš„æˆªæ­¢æ—¶é—´"""
        current_time = datetime.now()  # èŽ·å–å½“å‰ç³»ç»Ÿæ—¶é—´
        return current_time > self.expiration_date

    def get_remaining_days(self):
        """è®¡ç®—è·ç¦»è¿‡æœŸæ—¶é—´è¿˜æœ‰å¤šå°‘å¤©"""
        current_time = datetime.now()  # èŽ·å–å½“å‰ç³»ç»Ÿæ—¶é—´
        days_left = (self.expiration_date - current_time).days  # è®¡ç®—å‰©ä½™å¤©æ•°
        return days_left  # è¿”å›žå‰©ä½™çš„å¤©æ•°

