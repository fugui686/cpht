    def save_config(self, port, token):  # ä¿å­˜é…ç½®åˆ°æ–‡ä»¶
        config = ConfigParser()
        config['USER'] = {'Port': port, 'Token': token}
        with open('config.ini', 'w') as configfile:
            config.write(configfile)

    def load_config(self):  # ä»Žæ–‡ä»¶åŠ è½½é…ç½®
        config = ConfigParser()
        config.read('config.ini')
        if 'USER' in config:
            self.srk_port.setText(config['USER']['Port'])
            self.srk_token.setText(config['USER']['Token'])

    def print_accoun(self):  # é»˜è®¤èŽ·å–æ˜¨æ—¥æ•°æ®
        try:
            port = self.srk_port.text().strip()
            token = self.srk_token.text().strip()

            å½“å‰æ—¶é—´ = datetime.now()
            æ˜¨å¤© = (å½“å‰æ—¶é—´ - timedelta(days=1)).strftime('%Y-%m-%d')
            print(æ˜¨å¤©)

            headers = {
                'Cookie': token,
            }

            url = f'http://back.{port}.com/back/cards/openCardsCoinLog/openCardsCoinLogList'

            wb = Workbook()
            ws = wb.active
            ws.append(['ä¼šå‘˜è´¦å·', 'é‡‘é¢', 'æµæ°´', 'å¤‡æ³¨'])

            params = {
                'limit': '9999',
                'offset': '0',
                'options': '1',
                'username': '',
                'startTime': æ˜¨å¤©,
                'endTime': æ˜¨å¤©,
                'orderBy': 'winAmount desc',
                '_': '1729097486362',
            }

            response = requests.get(url, params=params, headers=headers, verify=False)
            if response.status_code == 200:
                data = response.json()['rows']

                for aa in data:
                    ä¼šå‘˜è´¦å· = aa.get('username')
                    æŠ•æ³¨é‡‘é¢ = aa.get('betsAmount')

                    æµæ°´ = 1
                    æ£‹ç‰Œå¾—æ„ = 'æ£‹ç‰Œå¾—æ„'

