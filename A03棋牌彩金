                            ws.append([ä¼šå‘˜è´¦å·, å½©é‡‘, æµæ°´, æ£‹ç‰Œå¾—æ„])
                        if 100000 <= æŠ•æ³¨é‡‘é¢ <= 299999:
                            å½©é‡‘ = 888
                            ws.append([ä¼šå‘˜è´¦å·, å½©é‡‘, æµæ°´, æ£‹ç‰Œå¾—æ„])
                        if 300000 <= æŠ•æ³¨é‡‘é¢ <= 499999:
                            å½©é‡‘ = 1888
                            ws.append([ä¼šå‘˜è´¦å·, å½©é‡‘, æµæ°´, æ£‹ç‰Œå¾—æ„])
                        if 500000 <= æŠ•æ³¨é‡‘é¢ <= 999999:
                            å½©é‡‘ = 3888
                            ws.append([ä¼šå‘˜è´¦å·, å½©é‡‘, æµæ°´, æ£‹ç‰Œå¾—æ„])
                        if 1000000 <= æŠ•æ³¨é‡‘é¢ <= 10000000:
                            å½©é‡‘ = 8888
                            ws.append([ä¼šå‘˜è´¦å·, å½©é‡‘, æµæ°´, æ£‹ç‰Œå¾—æ„])

